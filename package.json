{
  "name": "kross_app",
  "version": "2.0.0-alpha.0.9.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:clean": "cd android && gradlew clean && gradlew cleanBuildCache && cd ..",
    "babel:clean": "rm -rf node_modules/.cache/babel-loader/*",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "lint": "eslint .",
    "dev:ios-bash": "ENVFILE=.env.dev react-native run-ios --variant=devDebug --appIdSuffix=dev",
    "dev:android": "cp .env.dev .env && react-native run-android --variant=devDebug --appIdSuffix=dev",
    "dev:android-cmd": "copy .env.dev .env && react-native run-android --variant=devDebug --appIdSuffix=dev",
    "test:ios-bash": "ENVFILE=.env.test react-native run-ios --variant=testDebug --appIdSuffix=test",
    "test:android": "cp .env.test .env && react-native run-android --variant=qaDebug --appIdSuffix=test",
    "test:android-cmd": "copy .env.test .env && react-native run-android --variant=qaDebug --appIdSuffix=test",
    "prod:ios-bash": "ENVFILE=.env.production react-native run-ios --variant=prodDebug",
    "prod:android": "cp .env.production .env && npx react-native run-android --variant=prodDebug",
    "prod:android-cmd": "copy .env.production .env && npx react-native run-android --variant=prodDebug",
    "rnsemup": "rnsemup"
  },
  "dependencies": {
